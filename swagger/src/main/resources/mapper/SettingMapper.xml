<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.yicheng.dao.SettingDao">

    <select id="getSettingTemplate" parameterType="map" resultType="string">
    	SELECT  Template
    	FROM tb_Setting 
    	WHERE Type = #{type} and BizId=#{bizId}
    </select>
    
    
    
</mapper>
 